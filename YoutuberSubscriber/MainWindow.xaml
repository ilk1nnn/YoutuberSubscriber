<Window x:Class="YoutuberSubscriber.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YoutuberSubscriber"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    
    
    
    
    <Grid>



        <Grid.RowDefinitions>
            <RowDefinition Height="2"></RowDefinition>
            <RowDefinition ></RowDefinition>
        </Grid.RowDefinitions>


        <Grid Grid.Row="0">
            <Button Click="Button_Click" Content="Youtuber" FontSize="70" Foreground="White" FontWeight="Medium" Background="Red"></Button>
        </Grid>

        <Grid Grid.Row="1">
            <Button Content="Subscriber" FontSize="70" Foreground="White" FontWeight="Medium" Background="DimGray"></Button>
        </Grid>
        


    </Grid>
    
    
    
    
</Window>
