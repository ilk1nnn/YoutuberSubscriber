<Window x:Class="YoutuberSubscriber.SubscriberW"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YoutuberSubscriber"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="SubscriberW" Height="450" Width="800">
    
    
    
    <Grid>



        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>


        <Grid Grid.Row="0">
            <Button Content="Show All Posts" FontSize="70" Foreground="White" FontWeight="Medium" Background="Purple" Click="Button_Click_1"></Button>
        </Grid>

        <Grid Grid.Row="1">
            <Button Foreground="White"  Content="Show All Notifications" FontSize="70"  FontWeight="Medium" Background="DeepSkyBlue" Click="Button_Click"></Button>
        </Grid>



    </Grid>
    
    
    
</Window>
